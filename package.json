  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js",
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@s-ui/lint": "^3.39.0",
    "@s-ui/mockmock": "^2.0.0",
